<Window x:Class="client_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client_app"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="MainWindow" Height="490" Width="420" MaxHeight="490" MaxWidth="420">
    <Grid Loaded="Grid_Loaded">
        <Button Content="Назад" HorizontalAlignment="Left" Margin="22,415,0,0" VerticalAlignment="Top" Height="29" Width="144" Click="BackToLoginWindow"/>
        <DataGrid x:Name="dataGrid" AutoGenerateColumns="True" IsReadOnly="True" Margin="0,0,0,64" />
        <Button Content="Новый заказ" HorizontalAlignment="Left" Margin="252,415,0,0" VerticalAlignment="Top" Height="29" Width="144" Click="MoveToCreateOrderWindow"/>
    </Grid>
</Window>
